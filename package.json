{
  "dependencies": {
    "command-line-args": "5.2.1",
    "command-line-usage": "6.1.3",
    "megalodon": "4.1.0",
    "readline-sync": "1.4.10",
    "tslib": "2.4.0",
    "typescript": "4.8.4"
  },
  "devDependencies": {
    "@types/command-line-args": "5.2.0",
